     input3: '',\n      watcherFilterList: []\n    }\n  },\n  //Vue Hook\n  created() {\n    this.searchMethod()\n  },\n  watch: {\n    input3: {\n      handler() {\n        this.watcherFilterList =\n            this.classList\n                .filter(item => item.toLowerCase().includes(this.input3.toLowerCase()))\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.container {\n  margin: 0 auto;\n  padding: 30px;\n  max-width: 600px;  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n}\n.col {\n  margin-bottom: 2rem;\n  width: 50%;\n  height: 100%;\n}\ninput {\n  padding: 10px 6px;\n  margin: 20px 10px 10px 0;\n}\n</style>\n"]}]}