s":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Menu.vue","sourceRoot":"02_Shop/src/views","sourcesContent":["<template>\n  <div>\n    <h1>Unsere Spezialitäten</h1>\n    <div class=\"container\">\n      <div class=\"col\">\n        <h3>Hier nach ihrem Wunsch-Essen suchen</h3>\n        <input\n            type=\"text\"\n            placeholder=\"Search with watcher\"\n            v-model=\"input3\"\n        />\n        <ul>\n          <li v-for=\"(item, i) in watcherFilterList\" :key=\"i\">\n            {{ item }}\n          </li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      // Shared\n      classList: [\n        'Ceasar Salad', 'Green Salad', 'Mixed Salad', 'Tomato Salad',\n        'Risotto Mare e Monti', 'Risotto Frutti di Mare',\n        'Pizza Margherita', 'Pizza Prosciutto', 'Pizza Salame', 'Pizza Calzone',\n        'Desert: Tiramisù', 'Desert: Glacé', 'Desert: Hot Brownie'\n      ],\n      // Watcher\n      input3: '',\n      watcherFilterList: []\n    }\n  },\n  //Vue Hook\n  created() {\n    this.searchMethod()\n  },\n  methods: {\n    searchMethod(e) {\n      //console.log(e)\n      this.methodFilterList =\n          this.classList.filter(item =>\n              item.toLowerCase().includes(this.input.toLowerCase())\n          )\n    },\n  },\n  computed: {\n    computedList() {\n      return this.classList.filter(item => {\n        return item.toLowerCase().includes(this.input2.toLowerCase())\n      })\n    },\n  },\n  watch: {\n    input3: {\n      handler() {\n        this.watcherFilterList =\n            this.classList\n                .filter(item => item.toLowerCase().includes(this.input3.toLowerCase()))\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.container {\n  margin: 0 auto;\n  padding: 30px;\n  max-width: 600px;  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n}\n.col {\n  margin-bottom: 2rem;\n  width: 50%;\n  height: 100%;\n}\ninput {\n  padding: 10px 6px;\n  margin: 20px 10px 10px 0;\n}\n</style>\n"]}]}