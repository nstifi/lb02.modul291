e\"\n      style=\"max-width: 50rem;\"\n      class=\"mb-n4\"\n  >\n    <b-card-text>\n      <b-row class=\"my-1\">\n      <b-col sm=\"5\">\n        <label>My name is {{ $store.state.name }} and\n          I have {{ $store.state.totalCats }} cats.</label>\n      </b-col>\n      <b-col sm=\"2\">\n        <b-form-input v-model=\"newName\"></b-form-input>\n      </b-col>\n      <b-col sm=\"4\">\n        <b-button variant=\"primary\"\n                  @click=\"setName\" :disabled=\"!newName\">Update</b-button>\n      </b-col>\n      </b-row>\n      <Cat/>\n    </b-card-text>\n  </b-card>\n</b-container>\n</div>\n</template>\n\n\n<script>\nimport Cat from './components/Cat.vue'\nexport default {\n  name: 'app',\n  components: {\n    Cat\n  },\n  data() {\n    return {\n      newName:''\n    }\n  },\n  methods: {\n    setName() {\n      if(this.newName) {\n        this.$store.commit('setName', this.newName);\n        this.newName = '';\n      }\n    }\n  }\n}\n</script>\n"]}]}